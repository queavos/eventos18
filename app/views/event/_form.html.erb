<ul>
<% 
@event.errors.full_messages.each do |m|

%>
<li> <%= m %></li> 
<% end %>
</ul>

<p>
<%= e.label :user_id %> </br>
<%= e.collection_select(:user_id, User.all, :id, :email)  %>
</p>
<p>
<%= e.label :name %> </br>
<%= e.text_field :name, class: 'form-control'%>
</p>
<p>
<%= e.label :dateStart %> </br>
<%= e.date_select :dateStart %>
</p>
<p>
<%= e.label :dateEnd %> </br>
<%= e.date_select :dateEnd %>
</p>
<p>
<%= e.label :description %> </br>
<%= e.text_area :description,  class: 'form-control' %>
</p>
<p>
<%= e.label :status %>
<%= e.check_box :status %>
</p>
<p>
<%= e.submit "Crear", class: 'btn btn-primary' %>
</p>