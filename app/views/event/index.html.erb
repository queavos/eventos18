<h1>Event#index</h1>
<table class="table ">
	  <thead class="thead-dark">
	<tr><TH>Evento</TH><TH>Fecha inicio</TH><TH>Fecha fin</TH><TH>Descripcion</TH><TH>Estado</TH><TH>Encargado</TH><TH colspan="2"><%= link_to 'Nuevo', event_new_path %></TH></tr>
</thead>
	<tbody>
	<% @events.each do |f| %>
	
	<tr><TD><%= f.name %></TD>
		<TD><%= f.dateStart %></TD>
		<TD><%= f.dateEnd %></TD>
		<TD><%= f.description %></TD>
		<TD><%= f.status %></TD>
		<TD><%= f.user.username %></TD>
		<TD><%= link_to 'Editar', event_edit_path(f['id']) %></TD><TD>
<%= link_to 'Borrar', event_borrar_path(f['id']) %>
		</TD>
	</tr>
    <% end %>
    </tbody>
</table>
