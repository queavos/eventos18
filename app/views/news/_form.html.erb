<ul>
<% 
@new.errors.full_messages.each do |m|

%>
<li> <%= m %></li> 
<% end %>
</ul>

<p>
<%= e.label :title %> </br>
<%= e.text_field :title, class: 'form-control'%>
</p>
<p>
<%= e.label :body %> </br>
<%= e.text_area :body,  class: 'form-control' %>
</p>
<p>
<%= e.label :date %> </br>
<%= e.date_select :date %>
</p>
<p>
<%= e.label :event_id %> </br>
<%= e.collection_select(:event_id, Event.all, :id, :name)  %>
</p>
<p>
<%= e.submit "Crear", class: 'btn btn-primary' %>
</p>