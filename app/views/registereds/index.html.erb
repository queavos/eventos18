<p id="notice"><%= notice %></p>

<h1>Registereds</h1>

<table>
  <thead>
    <tr>
      <th>Regdate</th>
      <th>Status</th>
      <th>Checked</th>
      <th>User</th>
      <th>Event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registereds.each do |registered| %>
      <tr>
        <td><%= registered.regDate %></td>
        <td><%= registered.status %></td>
        <td><%= registered.checked %></td>
        <td><%= registered.user_id %></td>
        <td><%= registered.event_id %></td>
        <td><%= link_to 'Show', registered %></td>
        <td><%= link_to 'Edit', edit_registered_path(registered) %></td>
        <td><%= link_to 'Destroy', registered, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registered', new_registered_path %>
